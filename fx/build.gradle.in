plugins {
    id 'application'
    id 'org.openjfx.javafxplugin' version '0.0.14'
}

repositories {
    mavenCentral()
}

dependencies {
    implementation 'org.openjfx:javafx-controls:20.0.0.0'
    implementation 'org.openjfx:javafx-media:20.0.0.0'
}

javafx {
    version = "20.0.0.0"
    modules = [ 'javafx.controls', 'javafx.media' ]
}

application {
    mainClass = 'com.example.Main' // Replace with your main class
}
